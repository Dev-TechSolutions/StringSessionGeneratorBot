{
  "name": "String Session Generator Bot",
  "description": "Telegram Bot to Generate Pyrogram & Telethon String-Session Made By Team: Dev-TechSolutions",
  "logo": "https://telegra.ph/file/266aac5490d8f19d026bd.jpg",
  "keywords": [
    "telegram",
    "bot",
    "python",
    "pyrogram"
  ],
    "buildpacks": [{
    "url": "heroku/python"
  }],
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
  "repository": "https://github.com/Dev-TechSolutions/StringSessionGeneratorBot",
  "env": {
    "ENVIRONMENT": {
      "description": "Seting this to ANYTHING will Enable Heroku",
      "required": true,
      "value": "ANYTHING"
    },
    "API_ID": {
      "description": "Get this Value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "API_HASH": {
      "description": "Get this Value from my.telegram.org.",
      "required": true,
      "value": ""
    },
    "BOT_TOKEN": {
      "description": "Create a Telegram Bot & Enter Bot-Token Here",
      "required": true,
      "value": ""
    }
  }
}
